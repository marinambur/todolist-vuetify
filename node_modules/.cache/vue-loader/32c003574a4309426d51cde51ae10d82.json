-action>\n\n            <v-list-item-content>\n              <v-list-item-title   :class=\"{'text-decoration-line-through' : task.done}\">{{task.title}}</v-list-item-title>\n            </v-list-item-content>\n            <v-list-item-action>\n              <v-btn\n                  @click.stop=\"deleteTask(task.id)\"\n                  icon>\n                <v-icon color=\"primary lighten-1\">mdi-delete</v-icon>\n              </v-btn>\n            </v-list-item-action>\n          </template>\n        </v-list-item>\n        <v-divider></v-divider>\n      </div>\n\n\n    </v-list>\n  </div>\n</template>\n\n<script>\n\n  export default {\n    name: 'Todo',\n    data() {\n      return {\n        newTaskTitle: '',\n        tasks: [\n          {\n            id: 1,\n            title: 'Wake up',\n            done: true\n          }, {\n            id: 2,\n            title: 'Jump',\n            done: false\n          },\n          {\n            id: 3,\n            title: 'Eat',\n            done: false\n          },\n          {\n            id: 4,\n            title: 'Drink some coffee',\n            done: false\n          }\n        ]\n      }\n    },\n    methods: {\n      doneTask(id) {\n        let task = this.tasks.filter((task)=>task.id === id)[0];\n        task.done = !task.done;\n      },\n      deleteTask(id) {\n        this.tasks = this.tasks.filter((task)=>task.id !== id);\n      },\n      addTask() {\n      let newTask = {\n        id: Date.now(),\n        title: this.newTaskTitle,\n        done: false\n        };\n        this.tasks.push(newTask);\n        this.newTaskTitle = '';\n      }\n    }\n\n  }\n</script>\n"]}]}